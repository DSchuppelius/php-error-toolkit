{
    "fileHeaderComment.parameter": {
        "date": "YYYY-MM-DD",
        "commentbegin": "/*",
        "commentprefix": " *",
        "commentend": " */",
        "*": {
            "author": "Daniel JÃ¶rg Schuppelius",
            "web": "https://schuppelius.org",
            "license": "MIT License",
            "licenseuri": "https://opensource.org/license/mit"
        }
    },
    "fileHeaderComment.template": {
        "*": [
            "${commentbegin}",
            "${commentprefix} Created on   : ${date}",
            "${commentprefix} Author       : ${author}",
            "${commentprefix} Author Uri   : ${web}",
            "${commentprefix} Filename     : ${filename}",
            "${commentprefix} License      : ${license}",
            "${commentprefix} License Uri  : ${licenseuri}",
            "${commentend}"
        ]
    },
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "intelephense.format.braces": "k&r",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "notebook.defaultFormatter": "esbenp.prettier-vscode",
    "git.autofetch": true,
    "php.validate.executablePath": "C:\\Program Files\\PHP8.1\\php.exe",
    "redhat.telemetry.enabled": true,
    "prettier.requireConfig": true,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "phpunit.phpunit": "vendor\\bin\\phpunit.bat",
    "phpunit.environment": {
    },
    "remote.autoForwardPortsSource": "hybrid",
    "git.replaceTagsWhenPull": true,
    "notebook.formatOnSave.enabled": true,
    "prettier.documentSelectors": [
        "*.php"
    ],
    "php.executablePath": "c:\\program files\\php8.1\\php.exe",
    "workbench.startupEditor": "none",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.csv": "default"
    },
}
